$ exec git init
$ exec git add .
$ exec git commit -m "First commit"
$ exec git tag -a v0.1.0 -m "v0.1.0"

$ copy-file changes/build-sonatype.sbt build.sbt
> reload
> show version
> show releaseEarlyWith
-> releaseEarlyCheckRequirements

$ copy-file changes/build-bintray.sbt build.sbt
> reload
> show version
> show releaseEarlyWith
-> releaseEarlyCheckRequirements
